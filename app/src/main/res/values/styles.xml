<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="AppTheme" parent="@style/Theme.AppCompat.NoActionBar">
        <item name="colorPrimary">@color/myPrimaryColor</item>
        <item name="colorPrimaryDark">@color/myColorPrimaryDark</item>
        <item name="colorAccent">@color/myColorAccent</item>
    </style>
    <style name="AppThemeHome" parent="@style/Theme.AppCompat.NoActionBar">
        <item name="colorPrimary">@color/myPrimaryColor1</item>
        <item name="colorPrimaryDark">@color/myColorPrimaryDark1</item>
        <item name="colorAccent">@color/myColorAccent1</item>
    </style>
    <style name="MediaButton">
        <item name="android:background">@null</item>
        <item name="android:layout_width">@dimen/media_controller_button_width</item>
        <item name="android:layout_height">@dimen/media_controller_button_height</item>
    </style>
    <style name="MediaButton.Next" parent="@style/MediaButton">
        <item name="android:src">@android:drawable/ic_media_ff</item>
    </style>
    <style name="MediaButton.Play" parent="@style/MediaButton">
        <item name="android:src">@android:drawable/ic_media_play</item>
    </style>
    <style name="MediaButton.Previous" parent="@style/MediaButton">
        <item name="android:src">@android:drawable/ic_media_rew</item>
    </style>
    <style name="MediaText">
        <item name="android:textSize">@dimen/media_controller_text_size</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:paddingTop">@dimen/padding_medium</item>
    </style>
    <style name="MyCustomToolBarTheme" parent="@style/ThemeOverlay.AppCompat.Light">
        <item name="android:textColor">#fffdfcf9</item>
    </style>
</resources>
